{
	"env": {
		"node": true,
		"es6": true
	},
	"plugins": [
		"prettier"
	],
	"parser": "babel-eslint",
	"extends": [
		"eslint:recommended",
		"airbnb-base",
		"prettier"
	],
		"globals": {
		"Atomics": "readonly",
		"SharedArrayBuffer": "readonly"
	},
	"parserOptions": {
		"ecmaVersion": 2018,
		"sourceType": "module"
	},
	"rules": {
		"no-tabs": 0,
		"linebreak-style": [
			"error",
			"unix"
		],
		"quotes": [
			"error",
			"double"
		],
		"semi": [
			"error",
			"always"
		],
		"import/no-dynamic-require": 0,
		"global-require": 0
	}
}